{
  "name": "helloworld-types",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "helloworld-types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/helloworld-types",
        "main": "helloworld-types/src/index.ts",
        "tsConfig": "helloworld-types/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "helloworld-types/jest.config.ts"
      }
    }
  },
  "tags": []
}
