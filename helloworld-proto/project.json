{
  "name": "helloworld-proto",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "helloworld-proto/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
