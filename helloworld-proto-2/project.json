{
  "name": "helloworld-proto-2",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "helloworld-proto-2/src",
  "projectType": "library",
  "targets": {
    "build:proto": {
      "executor": "nx:run-commands",
      "options": {
        "command": "grpc_tools_node_protoc --js_out=import_style=commonjs_strict,binary:./ --grpc_out=grpc_js:./ helloworld.proto",
        "cwd": "helloworld-proto-2/src/lib"
      }
    },
    "build": {
      "dependsOn": ["build:proto"],
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/helloworld-proto-2",
        "main": "helloworld-proto-2/src/index.ts",
        "tsConfig": "helloworld-proto-2/tsconfig.lib.json",
        "assets": [],
        "format": ["cjs"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
